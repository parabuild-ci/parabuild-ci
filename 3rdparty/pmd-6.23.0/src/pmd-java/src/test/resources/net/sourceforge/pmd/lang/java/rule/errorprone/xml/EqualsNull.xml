<?xml version="1.0" encoding="UTF-8"?>
<test-data
    xmlns="http://pmd.sourceforge.net/rule-tests"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://pmd.sourceforge.net/rule-tests http://pmd.sourceforge.net/rule-tests_1_0_0.xsd">
    <test-code>
        <description><![CDATA[
object.equals(null), bad
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
public class Foo {
 void bar() {
  if (foo.equals(null)) {}
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
object == null, ok
     ]]></description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
public class Foo {
 void bar() {
  if (foo == null) {}
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
object.method().equals(null), bad
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
public class Foo {
 void bar() {
  if (foo.bar().equals(null)) {}
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description><![CDATA[
object.equals(null, other), ok
     ]]></description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
public class Foo {
 void bar() {
  if (foo.equals(null, other)) {}
 }
}
     ]]></code>
    </test-code>
    <test-code>
        <description>#1120 equalsnull false positive</description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
public class Foo {
  void bar() {
    if (container.getLabel(null).equals(grandChild.getCollectionName())) {
    }
  }
}
        ]]></code>
    </test-code>
</test-data>
