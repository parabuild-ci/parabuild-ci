<?xml version="1.0" encoding="UTF-8"?>
<test-data
    xmlns="http://pmd.sourceforge.net/rule-tests"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://pmd.sourceforge.net/rule-tests http://pmd.sourceforge.net/rule-tests_1_0_0.xsd">
    <test-code>
        <description><![CDATA[
1, iframe exists with src attribute
     ]]></description>
        <expected-problems>0</expected-problems>
        <code><![CDATA[
<html><body><iframe src="foo.html"></iframe></body></html>
     ]]></code>
        <source-type>jsp</source-type>
    </test-code>
    <test-code>
        <description><![CDATA[
2, iframe is missing src attribute
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
<html><body><iframe></iframe></body></html>
     ]]></code>
        <source-type>jsp</source-type>
    </test-code>
    <test-code>
        <description><![CDATA[
3, IFRAME is missing src attribute
     ]]></description>
        <expected-problems>1</expected-problems>
        <code><![CDATA[
<html><body><IFRAME></IFRAME></body></html>
     ]]></code>
        <source-type>jsp</source-type>
    </test-code>
</test-data>
