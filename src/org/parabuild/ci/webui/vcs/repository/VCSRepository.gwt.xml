<module rename-to="repository">

  <!-- Inherit GWT core -->

  <inherits name='com.google.gwt.user.User'/>
<!--  <inherits name='com.google.gwt.user.theme.clean.Clean'/>-->

  <!-- Enable debug ID. -->
  <inherits name="com.google.gwt.user.Debug"/>
  <set-property name="gwt.enableDebugId" value="true"/>

  <!-- Source path -->
  <source path="common"/>
  <source path="client"/>

  <!-- Parabuild client application -->
  <entry-point class="org.parabuild.ci.webui.vcs.repository.client.VCSRepositoryEntryPoint"/>

  <!-- RPC Service -->
  <servlet path="/service/repository" class="org.parabuild.ci.webui.vcs.repository.server.VCSRepositoryServiceImpl"/>
</module>
