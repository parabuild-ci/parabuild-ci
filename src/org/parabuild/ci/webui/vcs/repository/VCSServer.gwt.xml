<module rename-to="server">

  <!-- Inherit GWT core -->

  <inherits name='com.google.gwt.user.User'/>
  <inherits name='org.parabuild.ci.Common'/>
<!--  <inherits name='com.google.gwt.user.theme.clean.Clean'/>-->

  <!-- Enable debug ID. -->
  <inherits name="com.google.gwt.user.Debug"/>
  <set-property name="gwt.enableDebugId" value="true"/>

  <!-- Source path -->
  <source path="common"/>
  <source path="client"/>

  <!-- Parabuild client application -->
  <entry-point class='org.parabuild.ci.webui.vcs.repository.client.VCSServerEntryPoint'/>

  <servlet path="/service/server" class="org.parabuild.ci.webui.vcs.repository.server.VCSServerServiceImpl"/>
</module>
